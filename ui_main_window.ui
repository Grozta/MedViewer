/*QStackedWidget*/

/*QToolButton*/
QToolButton
{background:rgba(0,0,0,0); }
QToolButton:hover
{border:1px solid;border-top:0px;border-right:0px;border-left:0px;border-bottom-color:#2764c1;background:rgba(151,151,151,0.05);}
/*QPushButton*/
QPushButton
{background:#5E5E5E;border-radius:5px; }
QPushButton:hover
{border:1px solid #2764c1;}
/*QMenu*/
QMenu {
    background-color:qlineargradient(spread:pad,x1:0, y1:0, x2:0, y2:1,stop:0.7 #1a1d23,stop:1 #1a1d23);
	border:1px solid #282c34;
	border-radius:8px;
	padding-left:2px;
}
QMenu::item {
    font-size:8pt;
    color: #DCDCDC;
    background-color:transparent;;
    padding-right: 2px;
    margin: 2px 1px;
}
QMenu::item:selected {
    border:1px ridge #259bd9;
	border-radius:2px;
}
QMenu::icon:checked {
    background:#259bd9;
    position: absolute;
    top: 1px;
    right: 1px;
    bottom: 1px;
    left: 1px;
}
QMenu::icon:checked:selected {
    background-color : #259bd9;
    background-image: url(:/space_selected.png);
}
QListWidget
{
	background-color:transparent;   
	font-size:12px;
    color: #f2f2f2;
}
/*
tabelwidget*/
