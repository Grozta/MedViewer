/*QStackedWidget*/

/*QToolButton*/
QToolButton
{background:rgba(0,0,0,0); }
QToolButton:hover
{border:1px solid;border-top:0px;border-right:0px;border-left:0px;border-bottom-color:#2764c1;background:rgba(151,151,151,0.05);}
/*QPushButton*/
QPushButton
{background:#5E5E5E;border-radius:5px; }
QPushButton:hover
{border:1px solid #2764c1;}
/*QMenu*/
QMenu {
    background-color:qlineargradient(spread:pad,x1:0, y1:0, x2:0, y2:1,stop:0.7 #1a1d23,stop:1 #1a1d23);
	border:1px solid #282c34;
	border-radius:8px;
	padding-left:2px;
}
QMenu::item {
    font-size:8pt;
    color: #DCDCDC;
    background-color:transparent;;
    padding-right: 2px;
    margin: 2px 1px;
}
QMenu::item:selected {
    border:1px ridge #259bd9;
	border-radius:2px;
}
QMenu::icon:checked {
    background:#259bd9;
    position: absolute;
    top: 1px;
    right: 1px;
    bottom: 1px;
    left: 1px;
}
QMenu::icon:checked:selected {
    background-color : #259bd9;
    background-image: url(:/space_selected.png);
}
QListWidget
{
	background-color:transparent;   
	font-size:12px;
    color: #f2f2f2;
}
/*
tabelwidget*/
QTableWidget{
color:#DCDCDC;
background-color:qlineargradient(spread:pad,x1:0, y1:0, x2:0, y2:1,stop:0 #232323,stop:1 #121212 );
border:1px solid #242424;
}
QTableWidget::Item{border:0px solid rgb(255,0,0);border-bottom:1px solid #242424;}

/*选中item*/
QTableWidget::item:selected{
color:#DCDCDC;
background:qlineargradient(spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 #484848,stop:1 #383838);
}

/*
悬浮item*/
QTableWidget::item:hover{
background:rgba(0,0,0,0);
}
/*表头*/
QHeaderView::section{
text-align:center;
background:#5E5E5E;
padding:3px;
margin:0px;
color:#DCDCDC;
border:1px solid #242424;
border-left-width:0;
}
/*QLineEdit*/
QLineEdit {
	border: 1px solid #A0A0A0; /* 边框宽度为1px，颜色为#A0A0A0 */
	border-radius: 3px; /* 边框圆角 */
	padding-left: 5px; /* 文本距离左边界有5px */
	background-color: transparent; /* 背景颜色 */
	color: #DCDCDC; /* 文本颜色 */
	selection-background-color: #A0A0A0; /* 选中文本的背景颜色 */
	selection-color: #F2F2F2; /* 选中文本的颜色 */
	font-family: &quot;Microsoft YaHei&quot;; /* 文本字体族 */
	font-size: 10pt; /* 文本字体大小 */
}

/*horizontal ：水平QSlider*/
QSlider::groove:horizontal {
border: 0px solid #bbb;
}

/*1.滑动过的槽设计参数*/
QSlider::sub-page:horizontal {
 /*槽颜色*/
background: #2764c1;
 /*外环区域倒圆角度*/
border-radius: 2px;
 /*上遮住区域高度*/
margin-top:8px;
 /*下遮住区域高度*/
margin-bottom:8px;
/*width在这里无效，不写即可*/
}

/*2.未滑动过的槽设计参数*/
QSlider::add-page:horizontal {
/*槽颜色*/
background: #5E5E5E;
/*外环大小0px就是不显示，默认也是0*/
border: 0px solid #777;
/*外环区域倒圆角度*/
border-radius: 2px;
 /*上遮住区域高度*/
margin-top:9px;
 /*下遮住区域高度*/
margin-bottom:9px;
}
 
/*3.平时滑动的滑块设计参数*/
QSlider::handle:horizontal {
/*滑块颜色*/
background: rgb(193,204,208);
/*滑块的宽度*/
width: 5px;
/*滑块外环为1px，再加颜色*/
border: 1px solid rgb(193,204,208);
 /*滑块外环倒圆角度*/
border-radius: 2px; 
 /*上遮住区域高度*/
margin-top:6px;
 /*下遮住区域高度*/
margin-bottom:6px;
}

/*4.手动拉动时显示的滑块设计参数*/
QSlider::handle:horizontal:hover {
/*滑块颜色*/
background: rgb(193,204,208);
/*滑块的宽度*/
width: 10px;
/*滑块外环为1px，再加颜色*/
border: 1px solid rgb(193,204,208);
 /*滑块外环倒圆角度*/
border-radius: 5px; 
 /*上遮住区域高度*/
margin-top:4px;
 /*下遮住区域高度*/
margin-bottom:4px;
}
/*QGroupBox*/
QGroupBox {
    /* 背景渐变色*/
    background-color:qlineargradient(spread:pad,x1:0, y1:0, x2:0, y2:1,stop:0 #232323,stop:1 #121212 );
    /* 边框 */
    border: 1px solid #5E5E5E;

    /* 倒角 */
    border-radius: 5px;
padding: -16 6px 0 6px;

    /* 就像墙上挂着的两个相框,margin指的是相框与相框的距离
       padding指的是每个相框里照片与相框边框的距离*/
    margin-top: 8px; 
}
/* 标题设置 */
QGroupBox::title {
    /* 位置 */
    subcontrol-origin: margin;
    subcontrol-position: top left;
	left:10px; /* //线的偏离度 */
	margin-left: 0px;
	padding:0 1px;/* //文字在方框中位置的偏离度 */
    /* 内边框,上下和左右
    /* padding: 30px 15px; */

    /* 颜色 
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                      stop: 0 #FF0ECE, stop: 1 #FFFFFF);*/
}

QSplitter::handle {
	/*image:url(:/images/444.bmp);*/
	border-radius: 3px;
}
QSplitter::handle:hover {
	/*image:url(:/images/444.bmp);*/
	background-color:rgba(70,130,180,0.3);
	border-radius: 3px;
}

QSplitter::handle:horizontal {
	
	width:5px;
}

QSplitter::handle:vertical {
	
	width:5px;
}

QSplitter::handle:pressed {
	
	background-color:rgba(70,130,180,0.1);
 	border-radius: 3px;
}
QSpinBox {
    padding-top: 2px;
    padding-bottom: 2px;
	padding-left: 4px;
    padding-right: 15px;
    border: 1px solid rgb(64,64,64);
    border-radius: 3px;
	color: rgb(200,200,200);
    background-color: rgb(44,44,44);
	selection-color: rgb(235,235,235);
	selection-background-color: rgb(83,121,180);
	font-family: &quot;Microsoft Yahei&quot;;
	font-size: 10pt;
}

QSpinBox:hover {
    background-color: rgb(59,59,59);
}

QSpinBox::up-button { /* 向上按钮 */
	subcontrol-origin: border; /* 起始位置 */
	subcontrol-position: top right; /* 居于右上角 */
	border: none;
	width: 12px;
	margin-top: 2px;
	margin-right: 1px;
	margin-bottom: 0px;
}

QSpinBox::up-button:hover {
	border: none;
}

QSpinBox::up-arrow { /* 向上箭头，图片大小为8x8 */
	image: url(resources/btn_spin_up.png);
}

QSpinBox::up-arrow:hover {
	image: url(resources/btn_spin_up_hover.png);
}

QSpinBox::up-arrow:disabled, QSpinBox::up-arrow:off {
	image: url(resources/btn_spin_up_disable.png);
}

QSpinBox::down-button { /* 向下按钮 */
	subcontrol-origin: border;
	subcontrol-position: bottom right;
	border: none;
	width: 12px;
	margin-top: 0px;
	margin-right: 1px;
	margin-bottom: 2px;
}

QSpinBox::down-button:hover {
	border: none;
}

QSpinBox::down-arrow { /* 向下箭头 */
	image: url(resources/btn_spin_down.png);
}

QSpinBox::down-arrow:hover {
	image: url(resources/btn_spin_down_hover.png);
}

QSpinBox::down-arrow:disabled, QSpinBox::down-arrow:off {
	image: url(resources/btn_spin_down_disable.png);
}
