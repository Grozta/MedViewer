/*------------------------------------全局样式-----------------------------------------*/
QWidget{font: 12px;color:#DCDCDC;font-family:'微软雅黑';}
QFrame{border-radius:5px;}
/*QScrollBar*/
QScrollArea{border:none}
QScrollBar{border:none;background:transparent;}
QScrollBar::add-page,QScrollBar::sub-page{background:transparent;}
QScrollBar::add-line,QScrollBar::sub-line { background: none; }
QScrollBar::handle{ background:rgba(91, 100, 116, 0.5); border-radius: 3px; width:3px}
QScrollBar:vertical{width:7px;padding-left:2px;padding-right:2px;}
QScrollBar:horizontal{height:7px;padding-top:2px;padding-bottom:2px;}

/*QTabWidget*/
QTabWidget::pane{border:1px solid #1b1b1b;background:#1b1b1b;}

QTabBar::tab{		
	width:84px;					
	height:25;
	font: 16px;
	color:#DCDCDC; 					
	margin-top:0px; 			
	margin-right:5px;
	margin-left:5px;
	background:#1b1b1b;

}

QTabBar::tab:first:!selected {

	/*border-image: url(:/common/images/common/左_normal.png);*/
}
QTabBar::tab:selected {
	color:#2764c1; 	
	border:2px solid;
	border-top:0px;
	border-right:0px;
	border-left:0px;
	border-bottom-color:#2764c1;
}
QTabWidget::tab-bar {
alignment: left;
} 
